track altSequence
compositeTrack on
shortLabel Alt Map
longLabel Mappings to the alternate sequences/haplotypes
subGroup1 view Views location=Haplotype psl=Alignment chain=Chain net=Net
dragAndDrop subTracks
# sortOrder view=+
visibility hide
type bed 4
noInherit on
group map
priority 105
otherDb hg38
chainMinScore 1000
chainLinearGap medium
matrix 16 90,-330,-236,-356,-330,100,-318,-236,-236,-318,100,-330,-356,-236,-330,90
matrixHeader A, C, G, T
html altSequence

    track altSequenceViewLocation
    shortLabel Haplotype
    view location
    visibility pack
    parent altSequence
    noScoreFilter .

	track altLocations
        parent altSequenceViewLocation
        subGroups view=location
	color 32,32,190
	shortLabel Haplotypes
	longLabel Haplotypes to GRCh38 Reference Sequence
	html altSeqComposite
	priority 1
        url /cgi-bin/hgTracks?db=hg38&position=$$
        urlLabel Corresponding position:

    track altSequenceViewPsl
    shortLabel Alignment
    view psl
    visibility hide
    parent altSequence
    type psl
    indelDoubleInsert on
    indelQueryInsert on
    showDiffBasesAllScales .
    showDiffBasesMaxZoom 10000.0
    showCdsMaxZoom 10000.0
    showCdsAllScales .
    baseColorDefault itemBases
    pslSequence no
    baseColorUseSequence extFile seqHg19Patch10 extHg19Patch10

	track altSequenceLiftOver
        parent altSequenceViewPsl
        subGroups view=psl
	shortLabel Alignment
	longLabel GRCh38 Alternate Sequence Lift Over Alignments
	html altSeqComposite
	priority 3

    track altSequenceViewChain
    shortLabel Chain
    view chain
    visibility hide
    parent altSequence
    spectrum on

        track chainAltSequence
        parent altSequenceViewChain
        subGroups view=chain
        shortLabel Chain
        longLabel GRCh38 Alternate Sequence Chained Alignments
        type chain hg38
        html chainNet
	priority 4

    track altSequenceViewNet
    shortLabel Net
    view net
    visibility hide
    parent altSequence

        track netAltSequence
        parent altSequenceViewNet
        subGroups view=net
        shortLabel Net
        longLabel GRCh38 Alternate Sequence Alignment Net
        type netAlign hg38 chainAltSequence
        html chainNet
	priority 5
